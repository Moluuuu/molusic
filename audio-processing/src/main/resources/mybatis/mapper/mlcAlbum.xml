<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--绑定接口-->
<mapper namespace="com.molu.processing.mapper.MlcAlbumMapper">

    <select id="albumQueryAll" resultType="MlcAlbum">
        select *
        from `mlc_album`
    </select>

    <select id="albumQueryAllById" resultType="MlcAlbum">
        select *
        from `mlc_album`
        where id = #{id}
    </select>
</mapper>